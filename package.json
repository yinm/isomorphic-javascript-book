    "hapi": "^17.8.1",
    "nunjucks": "^3.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "cpx": "^1.5.0",
    "nodemon": "^1.18.9",
